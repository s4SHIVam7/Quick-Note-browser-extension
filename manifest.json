{
   "name": "Quick Note",
   "version": "1.0.1",
   "description": "Quick note saver",
   "manifest_version": 2,
   "background": {
     "scripts": [
       "js/background.js"
       ]
   },
   "browser_action": {
      "default_icon": "<logo>",
      "default_popup": "<popup>.html",
      "default_title": "Quick Note"
   },
   "icons": {
      "128": "<logo>.png",
      "48": "<logo>.png"
   },
   "permissions": [ "background", "unlimitedStorage" ]
}
